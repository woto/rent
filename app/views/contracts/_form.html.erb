<%= simple_form_for(@contract) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.association :renter %>
    <%= f.association :area %>
    <%= f.input :rate %>
    <%= f.input :date_start %>
    <%= f.input :date_end %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: 'btn-primary m-y-2' %>
  </div>
<% end %>
